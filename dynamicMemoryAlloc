#include<iostream>
using namespace std;

int main(){
    int *p;
    float *q;
    char *r;
    int *a;
    p = new int (10);
    q = new float(1.5);
    r = new char('A');
    a = new int[5];
    for(int i=0;i<5;i++){
        a[i]=i;
    }

    cout<<*p<<endl;
    cout<<*q<<endl;
    cout<<*r<<endl;
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
    return 0;
}
